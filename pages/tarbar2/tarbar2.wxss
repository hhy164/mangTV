/* pages/demotwo/demotwo.wxss */
.wrap{
  margin: 20rpx;
}
.title{
  font-size: 20px ;
  font-weight: 900;
  margin-top:20rpx;
  margin-bottom: 30rpx; 
  display: block;
}
.videolist{
  width: 97%;
  margin-left: 10rpx;
  margin-right: 10rpx;
  border-radius: 10px;
}
.part{
  margin:20rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;

}
.imagelist{
  width:60rpx; 
  height:60rpx;
  border-radius:100%; 
  overflow:hidden;
  margin-right: 20rpx;

}
.name{
  font-size: 15px ;
  font-weight: 900;
}
.icon{
  display: flex;
  justify-content: center;
  align-items: center;
}

.zan{
  width:20px; 
  height:20px;
  border-radius:100%; 
  overflow:hidden;
  margin-right: 30rpx;
}
.turn{
  width:17px; 
  height:17px;
  border-radius:100%; 
  overflow:hidden;
}